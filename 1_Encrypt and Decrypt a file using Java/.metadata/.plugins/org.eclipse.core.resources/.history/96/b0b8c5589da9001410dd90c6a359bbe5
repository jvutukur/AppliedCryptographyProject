

public class Encrypt extends ReadWriteOfFile  {
 
	
	
	public void blockEncrypt(String key32Bit,String plainText32Bit)
	{
		
		String s=new String();
		System.out.println(plainText32Bit.length());
		for(int i = 0; i < plainText32Bit.length(); i++)
		{
			s=s+(char)(plainText32Bit.charAt(i) ^ key32Bit.charAt(i));
		
		   
            cipherText=cipherText.concat(s);
            System.out.print(plainText32Bit.charAt(i) +""+ key32Bit.charAt(i)+" ");
	}
		System.out.println(plainText32Bit);
		System.out.println("length of plain text is:"+ plainText32Bit.length());
		 System.out.println("Lenght of encoded text is"+s.length());	
			
	      
	
	}
}

	

        
	
	
	
